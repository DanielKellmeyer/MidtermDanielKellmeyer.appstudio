let income = Number(inptIncome)
sbmtBtn.onclick = function () {
  if (income < 30000) {
    lblAnswer.value = 'With your income of' + income + ', you are in a tax bracket of 8%'
  } else if (income < 99999) {
    lblAnswer.value = 'With your income of' + income + ', you are in a tax bracket of 15%'
  } else if (income >= 99999) {
    lblAnswer.value = 'With your income of' + income + ', you are in a tax bracket of 25%'
  }
};

againBtn.onclick = function () {
  lblAnswer.value = 0
    inptIncome = 0
    sbmtBtn = 0
}
